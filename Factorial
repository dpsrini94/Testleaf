package Assingments;

public class Factorial {

	public static void main(String[] args) {
		int n=6;
		int i;
		
		int fact = 1;
		
		for (i = 1; i <= n; i++) {
			
			fact = fact*i;
			
		}
System.out.println("the factorial for the given number is:" + fact);
	}

}

output:
the factorial for the given number is:720
